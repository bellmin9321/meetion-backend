{
  "name": "meetion-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --exec ts-node -e ts,tsx,js,json ./src/bin/www",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npx tsc --watch\" \"nodemon ./src/bin/www\"",
    "watch-ts": "npx tsc --watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^7.0.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.2",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "helmet": "^6.0.1",
    "prettier": "2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "winston": "^3.8.2"
  }
